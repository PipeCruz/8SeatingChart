<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="600.0" prefWidth="890.0" stylesheets="@../css/Style.css"
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.cruz.controllers.Controller">
    <GridPane fx:id="griddy" alignment="CENTER" gridLinesVisible="true" layoutX="183.0" layoutY="1.0"
              prefHeight="600.0" prefWidth="700.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
        </rowConstraints>
    </GridPane>
    <Button fx:id="swapper" layoutX="44.0" layoutY="433.0" mnemonicParsing="false" onMouseClicked="#swap"
            prefHeight="25.0" prefWidth="88.0" text="Swap"/>
    <TextArea fx:id="tArea" layoutX="8.0" layoutY="65.0" prefHeight="250.0" prefWidth="160.0"/>
    <Button layoutX="44.0" layoutY="344.0" mnemonicParsing="false" onMouseClicked="#addStudentsFromList"
            text="Add Students"/>
    <Button fx:id="remover" layoutX="44.0" layoutY="522.0" mnemonicParsing="false" onMouseClicked="#remove"
            prefHeight="25.0" prefWidth="88.0" text="Remove"/>
    <MenuBar layoutY="1.0" prefHeight="25.0" prefWidth="88.0">
        <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" onAction="#help" text="Help"/>
            <MenuItem mnemonicParsing="false" onAction="#loadItUp" text="Import"/>
            <MenuItem mnemonicParsing="false" onAction="#export" text="Export"/>
            <MenuItem mnemonicParsing="false" onAction="#clear" text="Clear Seating Chart"/>
        </Menu>
        <Menu mnemonicParsing="false" text="null">
            <MenuItem mnemonicParsing="false" text="Delete"/>
        </Menu>
    </MenuBar>
</AnchorPane>
