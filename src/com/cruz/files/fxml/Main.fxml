<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="600.0" prefWidth="890.0" stylesheets="@../css/Style.css"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.cruz.controllers.Controller">
    <children>
        <GridPane fx:id="griddy" alignment="CENTER" gridLinesVisible="true" layoutX="183.0" layoutY="1.0"
                  prefHeight="600.0" prefWidth="700.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
        </GridPane>
        <Button fx:id="swapper" layoutX="59.0" layoutY="402.0" mnemonicParsing="false" onMouseClicked="#swap"
                prefHeight="25.0" prefWidth="75.0" text="Swap"/>
        <TextArea fx:id="tArea" layoutX="15.0" layoutY="60.0" prefHeight="249.0" prefWidth="163.0"/>
        <Button layoutX="48.0" layoutY="339.0" mnemonicParsing="false" onMouseClicked="#addStudentsFromList"
                text="Add Students"/>
        <MenuBar layoutY="1.0" prefHeight="25.0" prefWidth="88.0">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#help" text="Help"/>
                        <MenuItem mnemonicParsing="false" onAction="#loadItUp" text="Import"/>
                        <MenuItem mnemonicParsing="false" onAction="#export" text="Export"/>
                        <MenuItem mnemonicParsing="false" onAction="#clear" text="Clear Seating Chart"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="null">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Delete"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>
    </children>
</AnchorPane>
